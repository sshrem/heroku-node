div
    br
div.row
    form(name="roomsItems" )
        ul.col.s9.collapsible(data-collapsible="expandable" )
            li(ng-repeat="room in roomItems" track by $index)
                div.collapsible-header
                    | {{room.title | translate}}
                div.row.collapsible-body
                    ul.col.s3(ng-repeat="item in room.items")
                        input(id="radio_{{$parent.$index}}_{{$index}}"  name="group{{$parent.$index}}" type="radio" ng-model="selected[$parent.$index]" value="{{item.offeringId}}" ng-checked="isChecked($index)")
                        label(for="radio_{{$parent.$index}}_{{$index}}")
                        cl-image(public-id="{{item.imgCode}}"  width="50" crop="scale" format="png" quality="80" )
                        | {{item.name}}

button.btn.waves-effect.waves-light(type="submit" name="action" ui-sref="designs({projId: {{projId}}, aptId: {{atId}} })")
    | Submit
    i.material-icons.right send

