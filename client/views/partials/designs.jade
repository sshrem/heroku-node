div.row(ng-repeat="designRow in designsToShow")
    div.col.s12(ng-repeat="design in designRow")
        div.card.white.grey-text.text-darken-2
            div.card-content
                h6.hebrew {{design.title}}
            div.card-image
                cl-video#video.ds-imaging(public-id="{{design.imaging}}"  preload="none" controls autoplay)
